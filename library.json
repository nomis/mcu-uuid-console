{
	"name": "uuid-console",
	"description": "Console shell",
	"keywords": "console, shell, command line",
	"authors": [
		{
			"name": "Simon Arlott",
			"maintainer": true
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/nomis/mcu-uuid-console.git"
	},
	"version": "2.0.0",
	"license": "GPL-3.0-or-later",
	"homepage": "https://mcu-uuid-console.readthedocs.io/",
	"export": {
		"exclude": [
			".uuid-uk/*",
			"docs/build/*",
			"test/*",
			"*.sublime-*"
		]
	},
	"frameworks": [
		"arduino"
	],
	"dependencies": {
		"nomis/uuid-common": "^1.2.0",
		"nomis/uuid-log": ">=2.3.0,<4"
	},
	"build": {
		"flags": "-Wall -Wextra",
		"libLDFMode": "off"
